{
  "name": "wenv",
  "version": "3.1.1",
  "description": "Web Enviroment",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "postinstall": "node tools/node-sass-patch && install-app-deps && HOME=~/.electron-gyp npm rebuild node-sass --force",
    "start": "electron .",
    "release:dir": "npm run build --dir",
    "build": "build -p always",
    "release": "npm run dist && npm run build",
    "dist": "gulp --production",
    "dev": "gulp --watch",
    "rebuild": "electron-rebuild --ignore-scripts",
    "test": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.oml.ru:wenv/wenv3.git"
  },
  "keywords": [
    "wenv",
    "web",
    "enviroment"
  ],
  "author": "Mikhail Domozhirov",
  "license": "ISC",
  "dependencies": {
    "chokidar": "^2.0.4",
    "deep-object-diff": "^1.1.0",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.5",
    "extract-zip": "^1.6.7",
    "fs-extra": "^7.0.0",
    "get-port": "^4.0.0",
    "got": "^9.3.2",
    "ip": "^1.1.5",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.11",
    "node-sass": "^4.10.0",
    "opn": "^5.1.0",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "serve-index": "^1.9.1",
    "socket.io": "^2.1.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/extract-zip": "^1.6.2",
    "@types/fs-extra": "^5.0.4",
    "@types/get-port": "^4.0.0",
    "@types/got": "^9.2.1",
    "@types/ip": "0.0.32",
    "@types/js-cookie": "^2.2.0",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.32",
    "@types/koa-static": "^4.0.0",
    "@types/koa-views": "^2.0.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "^8.0.28",
    "@types/node-sass": "^3.10.32",
    "@types/opn": "^5.1.0",
    "@types/request": "^2.47.1",
    "@types/serve-index": "^1.7.29",
    "@types/socket.io": "^1.4.38",
    "del": "^3.0.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "electron-rebuild": "^1.8.2",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "typescript": "^3.0.3",
    "yargs": "^12.0.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "wenv": "./dist/index.js"
  },
  "pre-commit": [
    "build"
  ],
  "build": {
    "publish": [
      {
        "token": "817a7789e0a529e33697540c182af2941074d414",
        "provider": "github",
        "repo": "wenv",
        "owner": "domozhirov",
        "releaseType": "release",
        "publishAutoUpdate": true
      }
    ],
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "appId": "ru.megagroup.wenv3",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ],
      "identity": null,
      "icon": "./assets/icon.icns"
    },
    "directories": {
      "output": "build"
    }
  }
}
